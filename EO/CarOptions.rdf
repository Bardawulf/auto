    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/EO/CarOptions/PropertySpecification">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#ProductOrServiceModel"/>
