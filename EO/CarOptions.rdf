    <!-- https://spec.edmcouncil.org/auto/ontology/EO/CarOptions/CompleteCarModel -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/EO/CarOptions/CompleteCarModel">
        <rdfs:subClassOf rdf:resource="http://purl.org/goodrelations/v1#ProductOrServiceModel"/>
        <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/auto/ontology/EO/CarOptions/ConfigurationInfo"/>
